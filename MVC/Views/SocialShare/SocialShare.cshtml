<div class="@Model.CssClass">
    <ul class="list-inline social-share">
        @foreach (var button in Model.SocialButtons) {
            switch (button.ButtonName) {
            case "Facebook":
            <li class="share-facebook" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('https://www.facebook.com/sharer/sharer.php?app_id=309437425817038&amp;sdk=joey&amp;u=@shareUrl&amp;display=popup&amp;ref=plugin', 'Facebook','toolbar=no,width=550,height=180'); return false">
                    <svg focusable="false" class="icon icon-facebook2" focusable="false"><use xlink:href="#icon-facebook2"></use> </svg>
                </a>
            </li>
            break;

            case "Twitter":
            <li class="share-twitter" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('http://twitter.com/share?url=@shareUrl', 'Twitter', 'toolbar=no,width=550,height=450'); return false" >
                    <svg focusable="false" class="icon icon-twitter2" focusable="false"><use xlink:href="#icon-twitter2"></use> </svg>
                </a>
            </li>
            break;

            case "LinkedIn":
            <li class="share-linkedin" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('http://www.linkedin.com/shareArticle?mini=true&amp;url=@shareUrl&amp;title=@pageTitle', 'LinkedIn', 'toolbar=no,width=550,height=450', 'yes', 'center'); return false">
                    <svg focusable="false" class="icon icon-linkedin" focusable="false"><use xlink:href="#icon-linkedin"></use> </svg>
                </a>
            </li>
            break;

            case "GooglePlusOne":
            <li class="share-googleplus" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
			    <div class='g-plusone' data-size='small' data-annotation='none' data-callback="googleShareCallback"></div>
                <script type='text/javascript'>
                    (function () { var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true; po.src = 'https://apis.google.com/js/platform.js'; var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s); })();
                </script>
            </li>
            break;
                
            case "Blogger":
            <li class="share-bloggr" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('https://www.blogger.com/blogger.g?t&u=@shareUrl&n=@pageTitle', 'Blogger','toolbar=no,width=550,height=550'); return false" title="Share on Blogger">
                    <svg focusable="false" class="icon icon-blogger2" focusable="false"><use xlink:href="#icon-blogger2"></use> </svg>
                </a>
            </li>
            break;

            case "Delicious":
            <li class="share-delicious" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('https://delicious.com/save?v=5&noui&jump=close&url=@shareUrl&amp;title=@pageTitle', 'delicious','toolbar=no,width=550,height=550'); return false;">
                    <svg focusable="false" class="icon icon-delicious" focusable="false"><use xlink:href="#icon-delicious"></use> </svg>
                </a>
            </li>
            break;

            case "Tumblr":
            <li class="share-tumblr" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('http://www.tumblr.com/share', 'Tumblr','toolbar=no,width=900,height=550')"; title="Share on Tumblr" style="display: inline-block; text-indent: -9999px; overflow: hidden; width: 16px; height: 16px; background: url('https://platform.tumblr.com/v1/share_4.png') top left no-repeat transparent;">
                    <svg focusable="false" class="icon icon-tumblr2" focusable="false"><use xlink:href="#icon-tumblr2"></use> </svg>
                </a>
            </li>
            break;
        
            case "GoogleBookmarks":
            <li class="share-googlebookmark" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('https://www.google.com/bookmarks/mark?op=add&bkmk=@shareUrl&title=@pageTitle', 'Google bookmarks','toolbar=no,width=900,height=550'); return false" title="Share on GoogleBookmarks">
                    <svg focusable="false" class="icon icon-google-plus2" focusable="false"><use xlink:href="#icon-google-plus2"></use> </svg>
                </a>
            </li>
            break;

            case "Digg":
            <li class="share-digg" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('http://digg.com/submit?url=@shareUrl&title=@pageTitle', 'Digg','toolbar=no,width=550,height=550'); return false" title="Share on Digg">
                    <span class="sf-icon-digg"></span>
                </a>
            </li>
            break;

            case "MySpace":
            <li class="share-myspace" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('http://myspace.com/Modules/PostTo/Pages/?u=@shareUrl&t=@pageTitle', 'My Space','toolbar=no,width=550,height=550'); return false" title="Share on MySpace">
                    <span class="sf-icon-myspace"></span>
                </a>
            </li>
            break;

            case "StumbleUpon":
            <li class="share-stumbleupon" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('http://stumbleupon.com/submit?url=@shareUrl&title=@pageTitle', 'Stumble upon','toolbar=no,width=550,height=550'); return false" title="Share on StumbleUpon">
                    <svg focusable="false" class="icon icon-stumbleupon" focusable="false"><use xlink:href="#icon-stumbleupon"></use> </svg>
                </a>
            </li>
            break;

            case "Reddit":
            <li class="share-reddit" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a href="#" onclick="window.open('http://www.reddit.com/submit?newwindow=1&url=@shareUrl&title=@pageTitle', 'Reddit','toolbar=no,width=870,height=550'); return false" title="Share on Reddit">
                    <svg focusable="false" class="icon icon-reddit" focusable="false"><use xlink:href="#icon-reddit"></use> </svg>
                </a>
            </li>
            break;

            case "MailTo":
            <li class="share-mail" data-sf-role="socialShare" data-sf-socialshareoption="@button.ButtonName">
                <a onclick="window.open('mailto:?body=@shareUrl&subject=@pageTitle', 'MailTo', 'toolbar=no,width=550,height=550'); return false" title="Tell a friend">
                    <svg focusable="false" class="icon icon-mail"><use xlink:href="#icon-mail"></use> </svg>
                </a>
            </li>
            break;

            default:
            break;
            }
        }
        <li class="share-print">
            <a href="#" onclick="window.print();" title="Print Page">
                <svg focusable="false" class="icon icon-printer"><use xlink:href="#icon-printer"></use> </svg>
            </a>
        </li>
    </ul>
</div>
@Html.Script(Url.WidgetContent("Mvc/Scripts/SocialShare/social-share.js"), "bottom", false)